{
  "name": "HelixAI",
  "version": "1.0.0",
  "description": "Virtual Assistant for Laboratory Sciences",
  "engines": {
    "node": "6.9.5",
    "npm": "3.10.10"
  },
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "dev-start": "npm-run-all --parallel no-open:src",
    "no-open:src": "nodemon --exec babel-node tools/srcServerNoOpen",
    "dev-web": "nodemon --exec babel-node tools/distServer.js",
    "dev-clock": "nodemon --exec babel-node lib/clock/index.js",
    "dev-worker": "nodemon --exec babel-node lib/worker/index.js",
    "open:src": "babel-node tools/srcServerOpen",
    "createUser": "babel-node tools/createUser",
    "createClient": "babel-node tools/createClient",
    "createInventory": "babel-node tools/createInventory",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"src/**/*.test.js\" \"app/**/*.test.js\" \"app/controllers/alexa/**/*.test.js\" \"services/**/*.test.js\" \"jobs/**/*.test.js\"",
    "test-single": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test-models": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/models/**/*.test.js\"",
    "test-alexa": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/**/*.test.js\"",
    "test-abstract": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.abstract.controller.test.js\"",
    "test-cancel": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.cancel.controller.test.js\"",
    "test-help": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.help.controller.test.js\"",
    "test-introduction": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.introduction.controller.test.js\"",
    "test-inventory": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.inventory.controller.test.js\"",
    "test-launch": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.launch.controller.test.js\"",
    "test-lookup": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.lookup.controller.test.js\"",
    "test-note_taker": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.note_taker.controller.test.js\"",
    "test-order": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.order.controller.test.js\"",
    "test-page": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.page.controller.test.js\"",
    "test-protocol": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.protocol.controller.test.js\"",
    "test-quote": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.quote.controller.test.js\"",
    "test-recipe": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.recipe.controller.test.js\"",
    "test-reset": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.reset.controller.test.js\"",
    "test-sequence": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.sequence.controller.test.js\"",
    "test-stop": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.stop.controller.test.js\"",
    "test-thanks": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.thanks.controller.test.js\"",
    "test-video": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.video.controller.test.js\"",
    "test-whatsnew": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.whatsnew.controller.test.js\"",
    "test-whoami": "mocha --timeout 120000 --reporter progress tools/testSetup.js \"app/controllers/alexa/alexa.whoami.controller.test.js\"",
    "test-alexa:watch": "mocha --timeout 120000 --reporter progress --watch tools/testSetup.js \"app/controllers/alexa/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "test-jobs:watch": "mocha --timeout 120000 --reporter progress --watch \"jobs/**/*.test.js\" ",
    "clean-dist": "npm run remove-dist && mkdir dist && mkdir dist/admin && mkdir dist/web",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
